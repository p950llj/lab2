{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "packageRules": [
    {
      "matchPackagePatterns": ["^com.swedbank"],
      "allowedVersions": "/^[0-9]+\\.[0-9]+\\.[0-9]+$/"
    },
    {
      "matchPackagePatterns": ["^org.jdbi"],
      "matchUpdateTypes": ["patch", "minor"],
      "groupName": "All JDBI dependencies (non-major)"
    }
  ]
}
